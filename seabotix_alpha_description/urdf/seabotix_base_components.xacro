      <!-- robot name="OSU vLBV300 Volturnus" -->
      <xacro:property name="pi" value="3.1515926535897931" />
      <xacro:property name="deg2rad" value="${pi/180}" />

      <!-- Without trig, angles are a pain in the arse. Beam angle 30 -->
      <xacro:property name="dvl_beam_angle" value="${30/180*pi}" />
      <xacro:property name="dvl_pitch" value="0.361367" />
      <xacro:property name="dvl_yaw" value="0.387597" />


      <material name="green_v">
        <color rgba="0.5 0.9 0.3 1"/>
      </material>
      <material name="black_v">
        <color rgba="0 0 0 1"/>
      </material>
      <material name="red_v">
        <color rgba="1 0 0 1"/>
      </material>
      <material name="blue_v">
        <color rgba="0.2 0.4 0.6 1" />
      </material>
      <material name="grey_v">
        <color rgba=".3 .3 .3 1"/>
      </material>
      <material name="orange_v">
        <color rgba="1 .4 0 1" />
      </material>

      <link name="vehicle_base_link">
        <visual>
          <geometry>
            <box size="0.5 0.45 0.15"/>
          </geometry>
          <material name="green_v"/>
        </visual>
      </link>

      <link name="front_bumper">
        <visual>
          <geometry>
            <cylinder length="0.45" radius="0.075"/>
          </geometry>
          <material name="green_v"/>
        </visual>
      </link>
      <joint name="fbump_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="front_bumper"/>
        <origin rpy="${pi/2} 0 0" xyz="0.25 0 0"/>
      </joint>

      <link name="rear_bumper">
        <visual>
          <geometry>
            <cylinder length="0.45" radius="0.075"/>
          </geometry>
          <material name="green_v"/>
        </visual>
      </link>
      <joint name="rbump_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="rear_bumper"/>
        <origin rpy="${pi/2} 0 0" xyz="-0.25 0 0"/>
      </joint>

      <link name="l_arrow">
        <visual>
          <geometry>
            <box size="0.05 0.318 0.01"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="l_arrow_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="l_arrow"/>
        <origin rpy="0 0 ${pi/4}" xyz="0.05 0.095 0.075"/>
      </joint>

      <link name="r_arrow">
        <visual>
          <geometry>
            <box size="0.05 0.318 0.01"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="r_arrow_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="r_arrow"/>
        <origin rpy="0 0 ${-pi/4}" xyz="0.05 -0.095 0.075"/>
      </joint>


      <link name="sf_fan">
        <visual>
          <geometry>
            <cylinder length="0.04" radius="0.06"/>
          </geometry>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="base_to_sf_fan" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="sf_fan"/>
        <origin rpy="0 ${pi/2} 0.785" xyz="0.22 -0.12 -0.14"/>
      </joint>
      <link name="sf_thruster">
        <visual>
          <geometry>
            <cylinder length="0.1" radius="0.02"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="sf_fan_thruster_joint" type="fixed">
        <parent link="sf_fan"/>
        <child link="sf_thruster"/>
        <origin xyz="0 0 -0.06"/>
      </joint>

      <link name="pf_fan">
        <visual>
          <geometry>
            <cylinder length="0.04" radius="0.06"/>
          </geometry>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="base_to_pf_fan" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="pf_fan"/>
        <origin rpy="0 ${pi/2} -0.785" xyz="0.22 0.12 -0.14"/>
      </joint>
      <link name="pf_thruster">
        <visual>
          <geometry>
            <cylinder length="0.1" radius="0.02"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="pf_fan_thruster_joint" type="fixed">
        <parent link="pf_fan"/>
        <child link="pf_thruster"/>
        <origin xyz="0 0 -0.06"/>
      </joint>

      <link name="sa_fan">
        <visual>
          <geometry>
            <cylinder length="0.04" radius="0.06"/>
          </geometry>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="base_to_sa_fan" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="sa_fan"/>
        <origin rpy="0 ${-pi/2} -0.785" xyz="-0.22 -0.12 -0.14"/>
      </joint>
      <link name="sa_thruster">
        <visual>
          <geometry>
            <cylinder length="0.1" radius="0.02"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="sa_fan_thruster_joint" type="fixed">
        <parent link="sa_fan"/>
        <child link="sa_thruster"/>
        <origin xyz="0 0 -0.06"/>
      </joint>

      <link name="pa_fan">
        <visual>
          <geometry>
            <cylinder length="0.04" radius="0.06"/>
          </geometry>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="base_to_pa_fan" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="pa_fan"/>
        <origin rpy="0 -${pi/2} 0.785" xyz="-0.22 0.12 -0.14"/>
      </joint>
      <link name="pa_thruster">
        <visual>
          <geometry>
            <cylinder length="0.1" radius="0.02"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="pa_fan_thruster_joint" type="fixed">
        <parent link="pa_fan"/>
        <child link="pa_thruster"/>
        <origin xyz="0 0 -0.06"/>
      </joint>

      <link name="seabotix_bottle">
        <visual>
          <geometry>
            <cylinder length="0.26" radius="0.05"/>
          </geometry>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="sbottle_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="seabotix_bottle"/>
        <origin rpy="0 ${pi/2} 0" xyz="-0.05 0 -0.18"/>
      </joint>
<!-- 
      <link name="greensea">
        <visual>
          <geometry>
            <cylinder length="0.2" radius="0.04"/>
