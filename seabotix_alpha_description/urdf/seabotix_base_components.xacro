    <link name="base_link">
        <inertial>
        <mass value="${mass}" />
        <origin xyz="${cog}" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>
    <link name="world_link_x">
        <inertial>
        <mass value="0" />
        <origin xyz="0" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>
    <link name="world_link_y">
        <inertial>
        <mass value="0" />
        <origin xyz="0" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>
    <link name="world_link_z">
        <inertial>
        <mass value="0" />
        <origin xyz="0" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>
    <link name="world_link_roll">
        <inertial>
        <mass value="0" />
        <origin xyz="0" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>
    <link name="world_link_pitch">
        <inertial>
        <mass value="0" />
        <origin xyz="0" rpy="0 0 0"/>
        <inertia ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0" />
        </inertial>
    </link>

      <!-- Rigid body description of the base link -->
      <!-- <link name="${namespace}/base_link"> -->
      <link name="world_link_yaw">
        <!--
          Be careful to setup the coefficients for the inertial tensor,
          otherwise your model will become unstable on Gazebo
        -->
        <inertial>
          <mass value="0" />
          <origin xyz="0" rpy="0 0 0"/>
          <inertia ixx="0" ixy="0" ixz="0"
                   iyy="0" iyz="0"
                   izz="0" />
        </inertial>

