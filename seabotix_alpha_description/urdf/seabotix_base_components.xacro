      <link name="dvl">
        <visual>
          <geometry>
            <cylinder length="0.25" radius="0.06"/>
          </geometry>
          <material name="grey_v"/>
          <origin rpy="0 ${pi/2} 0" xyz="-0.125 0 0"/>
        </visual>
      </link>
      <joint name="dvl_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="dvl"/>
        <origin rpy="0 0 ${pi}" xyz="-0.23 -0.1 -0.29"/>
      </joint>

      <link name="dvl_head">
        <visual>
          <geometry>
            <sphere radius="0.06"/>
          </geometry>
          <material name="orange_v"/>
        </visual>
      </link>
      <joint name="dvl_head_link" type="fixed">
        <parent link="dvl"/>
        <child link="dvl_head"/>
        <origin rpy="0 0 ${-45*deg2rad}" xyz="0 0 0"/>
      </joint>

      <link name="dvl_altitude"/>
      <joint name="dvl_altitude_link" type="fixed">
        <parent link="dvl_head"/>
        <child link="dvl_altitude"/>
        <origin rpy="0 ${pi/2} 0" xyz="0 0 0"/>
      </joint>

      <link name="dvl_beam1">
        <visual>
          <geometry>
            <cylinder length="0.07" radius="0.015"/>
          </geometry>
          <origin rpy="0 ${pi/2} 0" xyz="0.035 0 0"/>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="dvl_beam1_link" type="fixed">
        <parent link="dvl_head"/>
        <child link="dvl_beam1"/>
        <origin rpy="0 ${pi/2-dvl_beam_angle} ${pi/2}" xyz="0 0 0"/>
      </joint>

      <link name="dvl_beam2">
        <visual>
          <geometry>
            <cylinder length="0.07" radius="0.015"/>
          </geometry>
          <origin rpy="0 ${pi/2} 0" xyz="0.035 0 0"/>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="dvl_beam2_link" type="fixed">
        <parent link="dvl_head"/>
        <child link="dvl_beam2"/>
        <origin rpy="0 ${pi/2-dvl_beam_angle} -${pi/2}" xyz="0 0 0"/>
      </joint>

      <link name="dvl_beam3">
        <visual>
          <geometry>
            <cylinder length="0.07" radius="0.015"/>
          </geometry>
          <origin rpy="0 ${pi/2} 0" xyz="0.035 0 0"/>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="dvl_beam3_link" type="fixed">
        <parent link="dvl_head"/>
        <child link="dvl_beam3"/>
        <origin rpy="0 ${pi/2-dvl_beam_angle} 0" xyz="0 0 0"/>
      </joint>

      <link name="dvl_beam4">
        <visual>
          <geometry>
            <cylinder length="0.07" radius="0.015"/>
          </geometry>
          <origin rpy="0 ${pi/2} 0" xyz="0.035 0 0"/>
          <material name="blue_v"/>
        </visual>
      </link>
      <joint name="dvl_beam4_link" type="fixed">
        <parent link="dvl_head"/>
        <child link="dvl_beam4"/>
        <origin rpy="0 ${pi/2-dvl_beam_angle} ${pi}" xyz="0 0 0"/>
      </joint>

      <link name="sonar">
        <visual>
          <geometry>
            <box size="0.22 0.13 0.1"/>
          </geometry>
          <material name="orange_v"/>
        </visual>
      </link>
      <joint name="sonar_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="sonar"/>
        <origin xyz="0.1 0.12 -0.28"/>
      </joint>

      <link name="gripper_pole">
        <visual>
          <geometry>
            <cylinder length="0.38" radius="0.015"/>
          </geometry>
          <origin rpy="0 ${pi/2} 0" xyz="0.19 0 0"/>
          <material name="black_v"/>
        </visual>
      </link>
      <joint name="gripper_pole_link" type="fixed">
        <parent link="vehicle_base_link"/>
        <child link="gripper_pole"/>
        <origin rpy="0 0.1 0.3" xyz="0.15 -0.15 -0.2"/>
      </joint>



    <joint name="x_displacement" type="prismatic">
      <parent link="base_link"/>
      <child link="world_link_x"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit effort="100" velocity="0.5" lower="-10" upper="10" /> <!-- Assume we are operating in 20x20x20m cube-->
    </joint>

    <joint name="y_displacement" type="prismatic">
      <parent link="world_link_x"/>
      <child link="world_link_y"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="100" velocity="0.5" lower="-10" upper="10" /> <!-- Assume we are operating in 20x20x20m cube-->
    </joint>

    <joint name="z_displacement" type="prismatic">
      <parent link="world_link_y"/>
      <child link="world_link_z"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="100" velocity="0.5" lower="-10" upper="10" /> <!-- Assume we are operating in 20x20x20m cube-->
    </joint>

    <joint name="roll_displacement" type="revolute">
      <parent link="world_link_z"/>
      <child link="world_link_roll"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit effort="100" velocity="${pi/10}" lower="${-pi/2}" upper="${pi/2}" /> <!-- Assume we can roll by pi/2-->
    </joint>

    <joint name="pitch_displacement" type="revolute">
      <parent link="world_link_roll"/>
      <child link="world_link_pitch"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
      <limit effort="100" velocity="${pi/10}" lower="${-pi/2}" upper="${pi/2}" /> <!-- Assume we can pitch by pi/2-->
    </joint>

    <joint name="yaw_displacement" type="continuous">
      <parent link="world_link_pitch"/>
      <child link="world_link_yaw"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
      <limit effort="100" velocity="${pi/10}"/> <!-- Assume we have no limit on yaw-->
    </joint>

    <!-- Add a link attaching the vehicle to the world base link -->
    <joint name="joint_vehicle_base_world" type="fixed">
      <!-- <parent link="${namespace}/base_link"/> -->
      <parent link="world_link_yaw"/>
      <child link="vehicle_base_link"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>